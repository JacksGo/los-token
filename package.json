{
  "name": "los-token",
  "version": "1.0.0",
  "description": "Lightweight, minimal session tokens using BLAKE2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacksGo/los-token.git"
  },
  "keywords": [
    "token",
    "blake",
    "session",
    "jwt"
  ],
  "author": "Jackson Golletz <jackson.golletz+los@gmail.com> (https://github.com/JacksGo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JacksGo/los-token/issues"
  },
  "homepage": "https://github.com/JacksGo/los-token#readme",
  "dependencies": {
    "blake2": "^4.0.2",
    "ms": "^2.1.2"
  },
  "devDependencies": {
    "@types/blake2": "^4.0.1",
    "@types/jest": "^26.0.23",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.3.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "files": [
    "/dist/**/*"
  ]
}
